{
  "compilerOptions": {
    "target": "ES2020",
    "module": "node16",
    "moduleResolution": "Node16",
    "jsx": "react-jsx",
    "noEmit": true,
    "allowJs": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "types": ["node", "@docusaurus/module-type-aliases"]
  },
  "include": [
    "src",
    "blog",
    "docusaurus.config.ts",
    "plugins",
    "theme",
    "src/themes/BlogListPage"
  ]
}
